Edit Your Project

<%= form_for @project do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :goal %>
  <%= f.text_field :goal %>
  <%= f.collection_select :category_id, @categories,:id,:name, include_blank: true %>
  <%= f.submit "Update!" %>
<%end%>
